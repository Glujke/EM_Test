{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=EM_Test.db"
  },
  "Using": [ "Serilog.Sinks.SQLite" ],
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "EM_Test.db",
          "autoCreateSqlTable": true
        }
      }
    ],
    "Properties": {
      "ApplicationName": "EM_Test API"
    }
  },
  "AllowedHosts": "*"
}
